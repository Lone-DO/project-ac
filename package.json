{
  "name": "ac-project",
  "description": "The Dynamic Animal Crossing Clock for your Phone/ Descktop",
  "version": "2.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lone-DO/animalcrossing.git"
  },
  "keywords": [
    "Animal",
    "Crossing",
    "AnimalCrossing",
    "Nintendo",
    "Clock",
    "OST"
  ],
  "author": "lone-do",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lone-DO/animalcrossing/issues"
  },
  "homepage": "http://lone-do.github.io/animalcrossing",
  "dependencies": {
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^2.2.1",
    "materialize-css": "^1.0.0",
    "node-sass-chokidar": "^1.4.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^7.5.2",
    "react-router-dom": "^7.5.2",
    "react-scripts": "^3.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-watch": "^5.0.1"
  },
  "scripts": {
    "start": "npm-run-all -p gulp start-react",
    "build": "npm-run-all -s build-css build-react",
    "gulp": "gulp",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "build-css": "gulp sass",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "ac-project"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 0.12.0"
  }
}
